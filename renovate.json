{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "packageRules": [
    {
      "enabled": false,
      "matchPackagePatterns": ["*"],
      "automerge": true
    }
  ],
  "vulnerabilityAlerts": {
    "enabled": true
  },
  "osvVulnerabilityAlerts": true,
  "ignoreTests": true,
  "platformAutomerge": true,
  "automergeType": "branch"

}
